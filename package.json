{
  "name": "git-pages",
  "version": "0.8.0",
  "description": "Run your own github-like static pages",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/kensho/git-pages/issues"
  },
  "bin": {
    "git-pages": "./bin/git-pages.js"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/kensho/git-pages",
  "scripts": {
    "test": "mocha --compilers es6:mocha-traceur -R spec src/**/*-spec.es6 src/**/*-spec.js",
    "start": "node bin/git-pages.js",
    "watch": "nodemon bin/git-pages.js"
  },
  "author": "Martin Camacho <martin@mcamac.com>",
  "contributors": [
    "Gleb Bahmutov <gleb.bahmutov@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "chdir-promise": "0.2.1",
    "check-more-types": "2.1.1",
    "express": "4.13.3",
    "ggit": "1.1.4",
    "gift": "0.6.1",
    "gitlog": "2.1.1",
    "jade": "1.11.0",
    "lazy-ass": "0.6.0",
    "marked": "0.3.5",
    "morgan": "1.6.1",
    "ncp": "2.0.0",
    "ng-alertify": "0.9.0",
    "ng-http-estimate": "0.6.0",
    "nopt": "3.0.4",
    "promised-exec": "1.0.1",
    "q": "2.0.3",
    "quote": "0.4.0",
    "ramda": "0.17.1",
    "update-notifier": "0.5.0"
  },
  "devDependencies": {
    "mocha": "2.3.3",
    "mocha-traceur": "2.1.0",
    "nodemon": "1.7.0",
    "pre-git": "0.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kensho/git-pages.git"
  },
  "pre-commit": [
    "npm test",
    "npm version"
  ]
}
